{
  "kind": "singleType",
  "collectionName": "abouts",
  "info": {
    "singularName": "about",
    "pluralName": "abouts",
    "displayName": "About Page",
    "description": "About us page content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
